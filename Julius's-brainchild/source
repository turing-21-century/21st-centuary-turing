def decrypt():
    x=input()
    hri=''
    for y in range(len(x)):

           z=ord(x[y])
           if((z>=65 and z<=77)or(z>=97 and z<=109)):
               z=z+13
               hri+=chr(z)
           elif((z>77 and z<=91)or(z>109 and z<=123)):
               z=z-13
               hri+=(chr(z))
           else:
                hri+=x[y]


    print(hri)

if __name__=="__main__":
    decrypt()
